if has("gui_running")
    set go=aAce              " 去掉难看的工具栏和滑动条
    set transparency=30      " 透明背景
    set guifont=Monaco:h13   " 设置默认字体为monaco
    set showtabline=2        " 开启自带的tab栏
    set columns=180          " 设置宽
    set lines=55             " 设置长
    set gfn=Monaco:h13
    colorscheme asmanian2
endif
set nocompatible
syntax on
set autoindent
set number
